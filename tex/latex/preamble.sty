\newcommand{\colvec}[2]{\left[ \begin{smallmatrix}
           #1 \\
           #2
         \end{smallmatrix} \right] }

\newcommand{\tbt}[4]{\left( \begin{smallmatrix}
		#1 & #2 \\
		#3 & #4
	\end{smallmatrix} \right) }
\newcommand{\Tbt}[4]{\left( \begin{matrix}
		#1 & #2 \\
		#3 & #4
	\end{matrix} \right) }

